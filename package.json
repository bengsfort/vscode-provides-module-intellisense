{
    "name": "provides-module-intellisense",
    "displayName": "@providesModule Intellisense",
    "description": "Adds intellisense for node modules that utilise @providesModule.",
    "version": "0.1.0",
    "publisher": "bengsfort",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:javascriptreact",
        "workspaceContains:package.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.cacheModules",
            "title": "providesModule Sense: Cache Modules"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}